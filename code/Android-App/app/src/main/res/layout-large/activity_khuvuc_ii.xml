        android:textColor="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/btn2kv7"
        app:layout_constraintEnd_toEndOf="@+id/btn2kv4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn2kv4"
