        android:background="@drawable/button2v2"
        android:textColor="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/btn2kv6"
        app:layout_constraintEnd_toEndOf="@+id/btn2kv1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn2kv1"
        app:layout_constraintTop_toTopOf="@+id/btn2kv6"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <ToggleButton
        android:id="@+id/btn2kv2"
        android:layout_width="70dp"
        android:layout_height="70dp"

        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button2v2"
        android:textColor="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/btn2kv1"
        app:layout_constraintStart_toEndOf="@+id/btn2kv1"
        app:layout_constraintTop_toTopOf="@+id/btn2kv1"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <ToggleButton
        android:id="@+id/btn2kv1"
        android:layout_width="70dp"
        android:layout_height="70dp"

        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button2v2"
        android:textColor="@android:color/transparent"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        tools:ignore="MissingConstraints" />

    <ToggleButton
        android:id="@+id/btn2kv10"
        android:layout_width="70dp"
        android:layout_height="70dp"

        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button2v2"
        android:textColor="@android:color/transparent"
        app:layout_constraintEnd_toStartOf="@+id/btn2kv8"
        app:layout_constraintStart_toEndOf="@+id/btn2kv7"
        app:layout_constraintTop_toBottomOf="@+id/btn2kv7"
        tools:ignore="MissingConstraints" />


    <ToggleButton
        android:id="@+id/btn2kv7"
        android:layout_width="70dp"
        android:layout_height="70dp"

        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button2v2"
        android:textColor="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="@+id/btn2kv3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn2kv3"
        app:layout_constraintTop_toBottomOf="@+id/btn2ALLlr"
        tools:ignore="MissingConstraints" />

    <ToggleButton
        android:id="@+id/btn2kv8"
        android:layout_width="70dp"
        android:layout_height="70dp"

        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
